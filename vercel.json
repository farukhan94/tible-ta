{
  "version": 2,
  "builds": [
    {
      "src": "api/index.php",
      "use": "vercel-php@0.7.2"
    }
  ],
  "routes": [
    {
      "src": "/(css|js|images)/(.*)",
      "dest": "public/$1/$2"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.php"
    }
  ],
  "buildCommand": "composer install --no-dev --optimize-autoloader --no-interaction",
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_KEY": "base64:onEtyFyPpA5OCljDZuPUw1zl6ASNoUXjRcTCcIs3z/k=",
    "SESSION_DRIVER": "cookie",
    "CACHE_DRIVER": "array",
    "SESSION_LIFETIME": "120",
    "SESSION_ENCRYPT": "false",
    "SESSION_COOKIE": "tible_session",
    "LOG_CHANNEL": "stderr"
  }
}